<!DOCTYPE html>
<html>
<head>
  <title>EtsyProductCatalog</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <nav class='nav_bar'>
    <div class="logo">
      <%= link_to 'TIY Catalog', root_path, class: 'logo' %>
      <%= link_to( image_tag('cart.png', size: '40x40', class: 'cart').html_safe, '#') %>
    </div>
  </nav>

<%= yield %>

</body>
</html>
